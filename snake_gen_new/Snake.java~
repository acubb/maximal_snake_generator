
public class Snake {
    String snake_string;

    public Snake () {
	this.snake_string = "";
    }

    public Snake (String snake_string) {
	this.snake_string = snake_string;
    }

    public String get_snake_str () {
	return snake_string;
    }

    public void add_char (char to_add) {
	snake_string += to_add;
    }
}
